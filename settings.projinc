<?xml version="1.0"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" DefaultTargets="all">

    <!--
    Project level settings.
    Can be overridden on the commandline by things like:
    msbuild /p:PGPATH=C:\postgresql-8.3.0

